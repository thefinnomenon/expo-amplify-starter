{
  "main": "./index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:mobile": "expo start:web --https --ios --android",
    "web:ios": "expo start:web --https --ios",
    "web:android": "expo start:web --https --android",
    "eject": "expo eject",
    "prepare": "husky install",
    "build": "eas build -p all",
    "build:ios": "eas build -p ios",
    "build:android": "eas build -p android",
    "build:dev": "eas build -p all --profile dev",
    "build:dev:ios": "eas build -p ios --profile dev",
    "build:dev:android": "eas build -p android --profile dev",
    "build:web": "expo build:web && npx serve web-build",
    "test": "jest",
    "test:dev": "jest --watch --changedSince=origin/master",
    "test:debug": "jest -o --watch",
    "test:updateSnapshots": "jest -u",
    "test:ci": "jest --coverage",
    "test:badges": "npm run test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "test:e2e": "detox test",
    "test:e2e:ios": "detox test --configuration ios",
    "test:e2e:android": "detox test --configuration android",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "expo": "~42.0.1",
    "expo-status-bar": "~1.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@expo/webpack-config": "^0.14.0",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-controls": "^6.3.7",
    "@storybook/addon-knobs": "^6.3.0",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/addon-storyshots": "^6.3.7",
    "@storybook/addon-storysource": "^6.3.7",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "detox": "^18.20.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": ">=6",
    "jest-circus": "^27.0.6",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "^42.1.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "storybook-addon-designs": "^6.0.1",
