{
  "main": "./index.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install",
    "build": "eas build -p all",
    "build:ios": "eas build -p ios",
    "build:android": "eas build -p android",
    "build:dev": "eas build -p all --profile dev",
    "build:dev:ios": "eas build -p ios --profile dev",
    "build:dev:android": "eas build -p android --profile dev",
    "test": "jest",
    "test:dev": "jest --watch --changedSince=origin/master",
    "test:debug": "jest -o --watch",
    "test:updateSnapshots": "jest -u",
    "test:ci": "jest --coverage",
    "test:badges": "npm run test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "test:e2e": "detox test",
    "test:e2e:ios": "detox test --configuration ios",
    "test:e2e:android": "detox test --configuration android"
  },
  "dependencies": {
    "expo": "~42.0.1",
    "expo-status-bar": "~1.0.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@expo/webpack-config": "^0.14.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-eslint": "^10.1.0",
    "detox": "^18.20.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": ">=6",
    "jest-circus": "^27.0.6",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "^42.1.0",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^26",
    "typescript": "~4.0.0"
  }
}
